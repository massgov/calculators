{
  "name": "Mayflower-react-starter",
  "homepage": "http://calculator.digital.mass.gov.s3-website-us-east-1.amazonaws.com/pfml/contribution/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@massds/mayflower-react": "^8.23.2",
    "history": "^4.7.2",
    "jquery": "^3.2.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.2",
    "numbro": "^2.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-input-range": "^1.3.0",
    "react-router-dom": "^4.2.2",
    "react-url-query": "^1.4.0"
  },
  "devDependencies": {
    "react-scripts": "2.1.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "1.14.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules/ --include-path ./node_modules/@massds/mayflower-react/es/assets/scss/ src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules/ --include-path ./node_modules/@massds/mayflower-react/es/assets/scss/ ./src -o ./src --watch",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint --fix .; exit 0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
